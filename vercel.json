{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "env": {
        "ALLOWED_AGE": "6"
    }
}
